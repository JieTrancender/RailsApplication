<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>

<% if current_user %>
    <li><%= link_to current_user.name, "#" %></li>
    <li><%= link_to "logout", logout_path, method: 'delete' %></li>
<% else %>
    <li><%= link_to "Login", login_path %></li>
    <li><%= link_to "Signup", signup_path %></li>
<% end %>
